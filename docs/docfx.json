{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "src/Responder/**.csproj"
                    ],
                    "cwd": ".."
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "properties": {
                "TargetFramework": "net5.0"
            }
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadata": {
            "_appTitle": "Phlank.Responder Documentation"
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/darkfx"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}